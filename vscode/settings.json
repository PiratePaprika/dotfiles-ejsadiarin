{
  "editor.fontFamily": "'JetBrainsMono Nerd Font', 'monospace', monospace",
  "window.menuBarVisibility": "toggle",
  "security.workspace.trust.untrustedFiles": "newWindow",
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.enabled": false,
  "workbench.sideBar.location": "left",
  "editor.minimap.side": "right",
  "editor.minimap.scale": 2,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(250, 240, 170, 0.5)",
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.normal": "block",
  "vim.insertModeKeyBindings": [
    {
      "before": ["k", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<Esc>"],
      "commands": [":nohl"]
    },
    {
      "before": ["<leader>", "b", "d"],
      "commands": [":bd"]
    },
    {
      "before": ["H"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["L"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["K"],
      "commands": [
        {
          "command": "editor.action.showHover"
        }
      ]
    },
    {
      "before": ["<leader>", "r", "n"],
      "commands": [
        {
          "command": "editor.action.rename"
        }
      ]
    },
    {
      "before": ["<leader>", "c", "r"],
      "commands": [
        {
          "command": "editor.action.rename"
        }
      ]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": [
        {
          "command": "editor.action.codeAction"
        }
      ]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "f", "r"],
      "commands": ["workbench.action.openRecent"]
    },
    {
      "before": ["g", "r"],
      "commands": ["findAllFileReferences"]
    },
    {
      "before": ["g", "i"],
      "commands": ["findImplementations"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-b>": false,
    "<C-S-l>": false,
    "<C-i>": false
  },
  "settings.cycle": [
    {
      "id": "zenPerfection", // must be unique
      "overrideWorkspaceSettings": true,
      "values": [
        // Note: use the same settings in each values object
        {
          "workbench.editor.showTabs": "none",
          "breadcrumbs.enabled": true,
          "workbench.statusBar.visible": false,
          "workbench.activityBar.location": "hidden"
        },
        {
          "workbench.editor.showTabs": "multiple",
          "workbench.statusBar.visible": true,
          "workbench.activityBar.location": "side"
        }
      ]
    }
  ],
  // Enable smooth scrolling in the editor
  "editor.smoothScrolling": true,
  "editor.lineNumbers": "relative",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.minimap.enabled": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  // "vscode-neovim.useWSL": true,
  // "vscode-neovim.neovimExecutablePaths.linux": "/home/exquisite/.neovim/squashfs-root/usr/bin/nvim",
  "editor.cursorSurroundingLines": 5,
  "workbench.layoutControl.enabled": false,
  "discord.detailsEditing": "{empty}",
  "discord.detailsIdling": "{empty}",
  "discord.lowerDetailsDebugging": "{empty}",
  "discord.lowerDetailsIdling": "{empty}",
  "discord.removeDetails": true,
  "discord.removeLowerDetails": true,
  "discord.removeRemoteRepository": true,
  "glassit.alpha": 220,
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.iconTheme": "vscode-icons",
  "sqltools.useNodeRuntime": false,
  "terminal.integrated.env.linux": {},
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "keyboard.dispatch": "keyCode",
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true,
  "git.openRepositoryInParentFolders": "never",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "sqltools.disableNodeDetectNotifications": true,
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "window.zoomLevel": 2,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.showTabs": "multiple",
  "breadcrumbs.enabled": true,
  "workbench.activityBar.location": "hidden",
  "workbench.statusBar.visible": false
}
